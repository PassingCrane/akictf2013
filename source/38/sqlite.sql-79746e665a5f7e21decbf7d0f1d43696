CREATE TABLE note (
    title      VARCHAR(32) NOT NULL,
    path       VARCHAR(255) NOT NULL,
    address    VARCHAR(15) NOT NULL,
    created_at TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
    super_secret_FLAG VARCHAR(255)
);
